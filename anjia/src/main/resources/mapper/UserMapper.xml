<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.meng.anjia.dao.UserDao">

    <!-- 插入用户-->
    <insert id="insertUser" >
        insert into user (username,password,salt,headUrl, email)
        values (#{username},#{password},#{salt}, #{headUrl},#{email})
    </insert>

</mapper>